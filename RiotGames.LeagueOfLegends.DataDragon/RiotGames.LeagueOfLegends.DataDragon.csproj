<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.1;net5.0;net6.0</TargetFrameworks>
    <LangVersion>latest</LangVersion>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <RootNamespace>RiotGames.LeagueOfLegends.DataDragon</RootNamespace>
    <PackageId>MikaelDui.RiotGames.LeagueOfLegends.DataDragon</PackageId>
    <Authors>Mikael DÃºi Bolinder</Authors>
    <Product>MikaelDui.RiotGames.LeagueOfLegends.DataDragon</Product>
