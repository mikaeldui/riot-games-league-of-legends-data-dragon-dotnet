    <PackageProjectUrl>https://github.com/mikaeldui/riot-games-league-of-legends-data-dragon-dotnet</PackageProjectUrl>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <PackageIcon>RiotGames.LeagueOfLegends.DataDragon.png</PackageIcon>
    <RepositoryUrl>https://github.com/mikaeldui/riot-games-league-of-legends-data-dragon-dotnet</RepositoryUrl>
    <PackageTags>LeagueOfLegends; DataDragon; LeagueOfLegendsDataDragon; RiotGames; LoL; LoLDataDragon; DD, LoLDD</PackageTags>
    <PackageLicenseExpression>Unlicense</PackageLicenseExpression>
    <AssemblyName>MikaelDui.RiotGames.LeagueOfLegends.DataDragon</AssemblyName>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\RiotGames.LeagueOfLegends.DataDragon.png">
